petname = []
while True:
    print("Enter Pet name below >>> ")
    name = input("Pet Name : ")
    if name == "Q":
        break
    petname.append(name)
print(f"Pet name's are : {petname}")

check = input("You want to check Pet name : ")

if check not in petname:
    print(f"Name is Not Presnent in Pet Name : {check}")
else:
    print(f"Name is Present in Pet Name : {check}")
    
    
